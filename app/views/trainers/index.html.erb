<div class="container">
  <div class="row">
    <h3>Projects</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @trainers.each do |trainer| %>
            <% if trainer != current_trainer %>
              <tr>
                <%= render trainer %>
              </tr>
            <% end %>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
